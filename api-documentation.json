{
  "openapi": "3.0.3",
  "info": {
    "title": "FIT FRAME API",
    "description": "API to control users of the Fit Frame Gym APP and document the routes that existed in the application.",
    "version": "0.0.1",
    "termsOfService": "https://google.com",
    "contact": {
      "name": "Dev Support",
      "email": "contato@example.com",
      "url": "https://google.com"
    },
    "license": {
      "name": "GPLv3",
      "url": "https://www.gnu.org/licenses/gpl-3.0.html"
    }
  },
  "externalDocs": {
    "url": "https://google.com",
    "description": "External Documentation"
  },
  "servers": [
    {
      "url": "API's URL",
      "description": "Test API"
    }
  ],
  "tags": [
    {
      "name": "Users",
      "description": "Users of the application"
    },
    {
      "name": "Trainings",
      "description": "Users of the application"
    },
    {
      "name": "Exercises",
      "description": "Users of the application"
    },
    {
      "name": "Weights",
      "description": "Users of the application"
    }
  ],
  "paths": {
    "/users": {
      "get": {
        "tags": ["Users"],
        "summary": "Get all users",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "post": {
        "tags": ["Users"],
        "summary": "Create a new User",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password_hash": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "409": {
            "description": "User with same email already exists",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/users/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "schema": {
            "type": "integer"
          },
          "required": true
        }
      ],
      "get": {
        "tags": ["Users"],
        "summary": "Get a user by id",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "404": {
            "description": "User not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "security": [
    {
      "auth": []
    }
  ],
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password_hash": {
            "type": "string"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "trainings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Training"
            }
          },
          "exercises": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Exercise"
            }
          }
        }
      },
      "Training": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "number_of_times": {
            "type": "number"
          },
          "active": {
            "type": "boolean"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "user_id": {
            "type": "string"
          },
          "exercises": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Exercise"
            }
          },
          "history": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Routine_Training"
            }
          }
        }
      },
      "Routine_Training": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "started_at": {
            "type": "string",
            "format": "date-time"
          },
          "finished_at": {
            "type": "string",
            "format": "date-time"
          },
          "training_id": {
            "type": "string"
          },
          "exercises_history": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Routine_Exercise"
            }
          }
        }
      },
      "Exercise": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "training": {
            "type": "object",
            "$ref": "#/components/schemas/Training"
          },
          "series": {
            "type": "number"
          },
          "repetitions": {
            "type": "number"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "user_id": {
            "type": "string"
          },
          "training_id": {
            "type": "string"
          },
          "current_weight": {
            "type": "string"
          },
          "weight_history": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Weight"
            }
          }
        }
      },
      "Routine_Exercise": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "started_at": {
            "type": "string",
            "format": "date-time"
          },
          "finished_at": {
            "type": "string",
            "format": "date-time"
          },
          "routine_training_id": {
            "type": "string"
          },
          "training": {
            "type": "object",
            "$ref": "#/components/schemas/Training"
          },
          "weight_id": {
            "type": "object",
            "$ref": "#/components/schemas/Weight"
          }
        }
      },
      "Weight": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "value": {
            "type": "string"
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "exercise_id": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "auth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
